{
    "name": "kolayik/auth",
    "description": "Kolay Auth",
    "keywords": ["laravel jwt auth", "laravel", "jwt", "json web token"],
    "type": "library",
    "authors": [
        {
            "name": "Kolay",
            "email": "dev@kolayik.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/auth": "8.*",
        "illuminate/contracts": "8.*",
        "illuminate/http": "8.*",
        "illuminate/support": "8.*"
    },
    "require-dev": {
        "illuminate/console": "8.*",
        "illuminate/database": "8.*",
        "illuminate/routing": "8.*",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KolayIK\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KolayIK\\Auth\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
