{
    "name": "kolayik/auth",
    "description": "Kolay Auth",
    "keywords": ["laravel jwt auth", "laravel", "jwt", "json web token"],
    "type": "library",
    "authors": [
        {
            "name": "Kolay",
            "email": "dev@kolayik.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "illuminate/auth": "^8.0|^9.0",
        "illuminate/contracts": "^8.0|^9.0",
        "illuminate/http": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "illuminate/console": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/routing": "^8.0|^9.0",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KolayIK\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KolayIK\\Auth\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
