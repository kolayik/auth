{
    "name": "kolayik/auth",
    "description": "Kolay Auth",
    "keywords": ["laravel jwt auth", "laravel", "jwt", "json web token"],
    "type": "library",
    "authors": [
        {
            "name": "Kolay IK",
            "email": "hakan.taskin@kolayik.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/auth": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "illuminate/contracts": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "illuminate/http": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*"
    },
    "require-dev": {
        "illuminate/console": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "illuminate/database": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "illuminate/routing": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "mockery/mockery": ">=0.9.9",
        "phpunit/phpunit": "~4.8 || ~6.0"
    },
    "autoload": {
        "psr-4": {
            "KolayIK\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KolayIK\\Auth\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
