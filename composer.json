{
    "name": "kolayik/auth",
    "description": "Kolay Auth",
    "keywords": ["laravel jwt auth", "laravel", "jwt", "json web token"],
    "type": "library",
    "authors": [
        {
            "name": "Kolay IK",
            "email": "hakan.taskin@kolayik.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/auth": "6.*",
        "illuminate/contracts": "6.*",
        "illuminate/http": "6.*",
        "illuminate/support": "6.*"
    },
    "require-dev": {
        "illuminate/console": "6.*",
        "illuminate/database": "6.*",
        "illuminate/routing": "6.*",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "KolayIK\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KolayIK\\Auth\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
